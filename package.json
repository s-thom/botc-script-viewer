{
  "name": "botc-script-viewer",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "setup": "wrangler types && node --experimental-strip-types setup/generate-script-schema.ts && node --experimental-strip-types setup/download-role-icons.ts && node --experimental-strip-types setup/generate-edition-scripts.ts",
    "validate": "node --experimental-strip-types setup/ensure-characters-in-number-store.ts",
    "dev": "astro dev",
    "build": "astro build",
    "full-build": "npm run setup && npm run build",
    "preview": "astro preview",
    "lint": "eslint .",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.9",
    "astro": "^5.14.1",
    "clsx": "^2.1.1",
    "qrcode-svg": "^1.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/css": "^0.11.1",
    "@eslint/js": "^9.36.0",
    "@eslint/markdown": "^7.3.0",
    "@types/node": "^24.6.0",
    "@types/qrcode-svg": "^1.1.5",
    "@types/umami": "^2.10.1",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.4.0",
    "json-schema-to-typescript": "^15.0.4",
    "p-queue": "^8.1.1",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.45.0",
    "wrangler": "^4.26.0"
  }
}
